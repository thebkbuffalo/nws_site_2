<div class="new_picture">
  <%= form_for LowYieldingWell.new, url: '/water_storage/low_yielding_wells', html: { multipart: true } do |f| %>
    <h3>Add A New Photo</h3>
    <%= f.file_field :low_yield_pic %>
    <br>
    <h3>Add A Tag Line</h3>
    <%= f.text_field :lyw_tag, class: 'form-control', rows: 8 %>
    <br>
    <%= f.submit %>
  <% end %>
</div>
