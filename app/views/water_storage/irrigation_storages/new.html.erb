<div class="new_picture">
  <%= form_for IrrigationStorage.new, url: '/water_storage/irrigation_storages', html: { multipart: true } do |f| %>
    <h3>Add A New Photo</h3>
    <%= f.file_field :irrigation_storage_pic %>
    <br>
    <h3>Add A Tag Line</h3>
    <%= f.text_field :irrigation_storage_tag, class: 'form-control', rows: 8 %>
    <br>
    <%= f.submit %>
  <% end %>
</div>
