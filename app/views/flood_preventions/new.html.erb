<div class='new_picture'>
  <%= form_for FloodPrevention.new, url: '/flood_preventions', html: { multipart: true } do |f| %>
    <h3>Add A New Photo</h3>
    <%= f.file_field :flood_pic %>
    <br>
    <h3>Add A New Tag</h3>
    <%= f.text_field :flood_prevention_tag, class: 'form-control', rows: 8 %>
    <br>
    <%= f.submit %>
  <% end %>
</di>
