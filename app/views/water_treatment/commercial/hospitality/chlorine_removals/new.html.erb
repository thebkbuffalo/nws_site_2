<div class="new_picture">
  <%= form_for ChlorineRemoval.new, url: '/water_treatment/commercial/hospitality/chlorine_removals', html: { multipart: true } do |f| %>
    <h3>Add A New Photo</h3>
    <%= f.file_field :chlorine_pic %>
    <br>
    <h3>Add A Tag Line</h3>
    <%= f.text_field :chlorine_tag, class: 'form-control', rows: 8 %>
    <br>
    <%= f.submit %>
  <% end %>
</div>
