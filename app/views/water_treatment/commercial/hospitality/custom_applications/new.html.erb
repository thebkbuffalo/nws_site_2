<div class="new_picture">
  <%= form_for CustomApplication.new, url: '/water_treatment/commercial/hospitality/custom_applications', html: { multipart: true } do |f| %>
    <h3>Add A New Photo</h3>
    <%= f.file_field :custom_app_pic %>
    <br>
    <h3>Add A Tag Line</h3>
    <%= f.text_field :custom_app_tag, class: 'form-control', rows: 8 %>
    <br>
    <%= f.submit %>
  <% end %>
</div>
