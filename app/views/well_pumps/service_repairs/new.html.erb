<div class="new_picture">
  <%= form_for ServiceRepair.new, url: '/well_pumps/service_repairs', html: { multipart: true } do |f| %>
    <h3>Add A New Photo</h3>
    <%= f.file_field :service_repair_pic %>
    <br>
    <h3>Add A Tag Line</h3>
    <%= f.text_field :service_repair_tag, class: 'form-control', rows: 8 %>
    <br>
    <%= f.submit %>
  <% end %>
</div>
