<div class="new_picture">
  <%= form_for WellInspection.new, url: '/well_pumps/well_inspections', html: { multipart: true } do |f| %>
    <h3>Add A New Picture</h3>
    <%= f.file_field :well_inspection_pic %>
    <br>
    <h3>Add A Tag Line</h3>
    <%= f.text_field :well_inspection_tag, class: 'form-control', rows: 8 %>
    <br>
    <%= f.submit %>
  <% end %>
</div>
