<h1>TRENCHING</h1>

<% @trenchings.each do |trench| %>
  <%= image_tag(trench.trench_pic.url(:medium)) %>
  <% if current_user %>
    <h4><%= link_to 'DELETE', "/well_pumps/commercial/trenchings/#{trench.id}", method: :delete, data: { confirm: 'Are you sure?' } %></h4>
  <% end %>
<% end %>

<% if current_user %>
  <h4><%= link_to 'New Picture', '/well_pumps/commercial/trenchings/new' %></h4>
<% end %>
<h1 class='back'><%= link_to 'Back', '/well_pumps/commercial' %></h1>
