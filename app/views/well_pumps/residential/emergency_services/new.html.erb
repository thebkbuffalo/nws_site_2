<div class='new_picture'>
  <%= form_for EmergencyService.new, url: '/well_pumps/residential/emergency_services', html: { multipart: true } do |f| %>
    <h3>Add A New Picture</h3>
    <%= f.file_field :emergency_pic %>
    <br>
    <h3>Add A Tag</h3>
    <%= f.text_field :es_tag, class: 'form-control', rows: 8 %>
    <br>
    <%= f.submit %>
  <% end %>
</div>
