<div class="new_picture">
  <%= form_for NewServiceLine.new, url: '/well_pumps/residential/new_service_lines', html: { multipart: true } do |f| %>
    <h3>Add A New Picture</h3>
    <%= f.file_field :nsl_pic %>
    <br>
    <h3>Add A Tag</h3>
    <%= f.text_field :nsl_tag, class: "form-control", rows: 8 %>
    <br>
    <%= f.submit %>
  <% end %>
</div>
