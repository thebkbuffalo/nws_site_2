<div class="new_picture">
  <%= form_for RepairConversionPitRepair.new, url: '/well_pumps/residential/repairs_conversions_pit_repairs', html: { multipart: true } do |f| %>
    <h3>Add A New Picture</h3>
    <%= f.file_field :rcpr_pic %>
    <br>
    <h3>Add A Tag Line</h3>
    <%= f.text_field :rcpr_tag, class: 'form-control', rows: 8 %>
    <br>
    <%= f.submit %>
  <% end %>
</div>
