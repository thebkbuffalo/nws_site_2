<div class='new_picture'>
  <%= form_for WaterTesting.new, url: '/water_testings', html: { multipart: true } do |f| %>
    <h3>Add A New Photo</h3>
    <%= f.file_field :water_testing_pic %>
    <br>
    <h3>Add A New Tag</h3>
    <%= f.text_field :water_testing_tag, class: 'form-control', rows: 8 %>
    <br>
    <%= f.submit %>
  <% end %>
</div>
